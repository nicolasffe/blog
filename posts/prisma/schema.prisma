datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}


// posts/prisma/schema.prisma
model Post {
  id        String   @id @default(cuid())
  title     String
  createdAt DateTime @default(now())
  likes     Int      @default(0) // GARANTA QUE ESTA LINHA EXISTA
}